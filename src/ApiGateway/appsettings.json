{
  "Services": {
    "Name": "ApiGateway",
    "ApiGateway": {
      "Name": "ApiGateway",
      "Uri": "http://localhost:5000"
    },
    "AuthServer": {
      "Name": "AuthServer",
      "Uri": "http://localhost:5100"
    },
    "EventService": {
      "Name": "EventService",
      "Uri": "http://localhost:5200"
    },
    "GameService": {
      "Name": "GameService",
      "Uri": "http://localhost:5300"
    },
    "PaymentService": {
      "Name": "PaymentService",
      "Uri": "http://localhost:5400"
    }
  },
  "Logging": {
