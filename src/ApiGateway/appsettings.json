{
  "Service": {
    "Name": "AuthServer"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ConsoleEnabled": true,
    "Elk": {
      "Enabled": false,
      "Uri": "http://localhost:9200"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Authority": "http://localhost:5100",
    "ProviderKey": "IdentityApiKey",
    "Secret": "my_very_very_very_very_long_long_secret"
  },
  "Ocelot": {
    "Folder": "Routes/localhost"
  }
}
