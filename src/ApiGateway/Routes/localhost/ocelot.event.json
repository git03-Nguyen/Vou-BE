{
  "Routes": [
    // HealthCheck - public
    {
      "SwaggerKey": "eventservice",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5100
        }
      ],
      "DownstreamPathTemplate": "/",
      "UpstreamPathTemplate": "/EventService",
      "UpstreamHttpMethod": [ "GET" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "15s",
        "PeriodTimespan": 1,
        "Limit": 100
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": null,
        "AllowedScopes": []
      }
    },
    // AdminController
    {
      "SwaggerKey": "eventservice",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5100
        }
      ],
      "DownstreamPathTemplate": "/api/1/Admin/{everything}",
      "UpstreamPathTemplate": "/EventService/api/1/Admin/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "15s",
        "PeriodTimespan": 1,
        "Limit": 100
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "IdentityApiKey",
        "AllowedScopes": [ "service_scope" ]
      }
    }
  ]
}